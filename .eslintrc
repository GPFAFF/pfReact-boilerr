{
  "parser": "babel-eslint",
  "rules": {
    "no-unused-vars": 0,
    "max-len": [1, 120, 2, {
      "ignoreComments": true
    }],
    "prop-types": [1],
    "react/jsx-filename-extension": [1, {
      "extensions": [".js", ".jsx"]
    }]
  },
  "settings": {
    "react": {
      "version": "^16.6.1"
    }
  },
  "plugins": [
    "react"
  ],
  "extends": ["airbnb-base"]
}